{
	"name": "branham-chat",
	"workspaces": [
		"client",
		"server"
	],
	"private": true,
	"scripts": {
		"server:dev": "cd server && cargo run",
		"client:dev": "cd client && yarn dev",
		"server:start": "cd server && cargo run --release",
		"client:build": "cd client && yarn build",
		"dev": "run-p *:dev",
		"start": "run-s client:build server:start"
	},
	"devDependencies": {
		"eslint": "^8.11.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.0.0",
		"npm-run-all": "^4.1.5",
		"process": "^0.11.10"
	}
}
